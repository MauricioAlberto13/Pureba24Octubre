@{
}
@model ML.Usuario


<h1>GetALL</h1>



<table>

    <tr>
        <th>Id</th>
        <th>Nombre</th>
        <th>Apellidos</th>
        <th>Edad</th>
        <th>Fecha de Nacimiento </th>

    </tr>
    @foreach (ML.Usuario usuario in Model.Usuarios)
    {
    <tr>
        
            <td> @usuario.IdUsuario</td>
            <td> @usuario.Nombre</td>
            <td> @usuario.Apellidos</td>
            <td> @usuario.Edad</td>
            <td> @usuario.Fecha</td>
       
    </tr>
        }
</table>
